# NecroNet Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Backend API URL (required)
# Local development:
NEXT_PUBLIC_API_URL=http://localhost:8000
# Production (Render):
# NEXT_PUBLIC_API_URL=https://necronet-backend.onrender.com

# S3 Storage Configuration (optional - for direct URL construction)
NEXT_PUBLIC_S3_BUCKET=necronet-artifacts
NEXT_PUBLIC_S3_REGION=us-east-1
# For S3-compatible services (DigitalOcean Spaces, Wasabi, etc.):
# NEXT_PUBLIC_S3_ENDPOINT=https://your-s3-compatible-endpoint.com
