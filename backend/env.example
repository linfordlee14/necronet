# NecroNet Backend â€” Environment Variables
# Copy this to .env and fill in your values

# Supabase PostgreSQL
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here

# AWS S3 (or S3-compatible: DigitalOcean Spaces, Wasabi, LocalStack)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_S3_BUCKET=necronet-artifacts
AWS_S3_REGION=us-east-1
AWS_S3_ENDPOINT=  # Leave empty for AWS; set for S3-compatible (e.g., https://spaces.digitaloceanspaces.com)

# ElevenLabs TTS (optional; fallback to text if not set)
ELEVENLABS_API_KEY=your-api-key
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM  # Example: "Aria" voice

# FastAPI
PORT=8000
ENVIRONMENT=development  # or "production"
